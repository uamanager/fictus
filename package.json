  "scripts": {
    "nx": "nx",
    "prepare": "husky install",
    "commit": "commit",
    "semantic-release": "semantic-release",
    "lint": "nx run-many --target=lint --all --parallel",
    "test": "nx run-many --target=test --all --parallel",
    "test:ci": "nx run-many --target=test --all --parallel --configuration=ci",
    "build:all": "nx run-many --target=build --all --parallel",
    "build:all:prod": "nx run-many --target=build --all --parallel --configuration=production",
    "build:cli": "nx run cli:build",
    "build:cli:prod": "nx run cli:build:prod",
    "build:schematics": "nx run schematics:build",
    "build:schematics:prod": "nx run schematics:build:prod",
    "build:cypress": "nx run cypress:build",
    "build:cypress:prod": "nx run cypress:build:prod",
    "build:server": "nx run server:build",
    "build:server:prod": "nx run server:build:prod",
    "build:db": "nx run db:build",
    "build:db:prod": "nx run db:build:prod",
    "format": "nx format:write"
  },
  "dependencies": {
    "@types/express": "4.17.17",
    "express": "4.18.2",
    "reflect-metadata": "0.1.13",
    "tslib": "2.5.0"
  },
